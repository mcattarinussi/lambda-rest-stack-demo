{
  "name": "lambda-rest-stack-demo",
  "version": "0.1.0",
  "engines": {
    "node": "12",
    "npm": ">=6.12"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "tsc --noEmit && eslint --max-warnings 0 --ext ts src/**",
    "lint:fix": "eslint --fix --ext ts src/**",
    "test": "jest",
    "cdk:synth": "npm run build && cdk synth",
    "cdk:deploy": "npm run build && cdk deploy",
    "cdk:diff": "npm run build && cdk diff"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.27.0",
    "@aws-cdk/aws-apigateway": "^1.27.0",
    "@aws-cdk/aws-lambda": "^1.27.0",
    "@aws-cdk/core": "1.27.0",
    "@types/aws-lambda": "^8.10.46",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "aws-cdk": "1.27.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "ts-jest": "^24.1.0",
    "typescript": "~3.7.2"
  }
}